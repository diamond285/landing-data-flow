{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport api from \"@/service/api\";\nexport default {\n  name: \"sign-in-component\",\n  data() {\n    return {\n      email: \"\",\n      password: \"\"\n    };\n  },\n  mounted() {\n    localStorage.removeItem(\"auth_user\");\n  },\n  methods: {\n    login() {\n      if (this.email.length !== 0 && this.password.length !== 0) {\n        api.authorization.login({\n          data: {\n            email: this.email,\n            password: this.password\n          }\n        }).then(response => {\n          const data = response.data;\n          localStorage.setItem(\"type\", data.type);\n          localStorage.setItem('auth_user', data.token);\n          this.$router.push(\"/\");\n        }).catch(() => {\n          alert(\"Неправильная почта или пароль\");\n        });\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";AAmBA,OAAOA,GAAE,MAAO,eAAe;AAE/B,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,OAAO,GAAG;IACRC,YAAY,CAACC,UAAU,CAAC,WAAW;EACrC,CAAC;EACDC,OAAO,EAAE;IACPC,KAAK,GAAG;MACP,IAAI,IAAI,CAACN,KAAK,CAACO,MAAK,KAAM,KAAK,IAAI,CAACN,QAAQ,CAACM,MAAK,KAAM,CAAC,EAAE;QACzDV,GAAG,CAACW,aAAa,CAACF,KAAK,CAAC;UACtBP,IAAI,EAAE;YACJC,KAAK,EAAE,IAAI,CAACA,KAAK;YACjBC,QAAQ,EAAE,IAAI,CAACA;UACjB;QACF,CAAC,CAAC,CAACQ,IAAI,CAAEC,QAAQ,IAAK;UACpB,MAAMX,IAAG,GAAIW,QAAQ,CAACX,IAAI;UAC1BI,YAAY,CAACQ,OAAO,CAAC,MAAM,EAAEZ,IAAI,CAACa,IAAI,CAAC;UACvCT,YAAY,CAACQ,OAAO,CAAC,WAAW,EAAEZ,IAAI,CAACc,KAAK,CAAC;UAC7C,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG;QACvB,CAAC,CAAC,CAACC,KAAK,CAAC,MAAM;UACbC,KAAK,CAAC,+BAA+B;QACvC,CAAC;MACH;IACD;EACF;AACF","names":["api","name","data","email","password","mounted","localStorage","removeItem","methods","login","length","authorization","then","response","setItem","type","token","$router","push","catch","alert"],"sourceRoot":"","sources":["/Users/baur/WebstormProjects/docs-flow-front/src/components/signin/signin.vue"],"source